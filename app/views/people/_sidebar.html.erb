<aside class="sidebar">
  <div class="row">
    <div class="col-xs-6">
      <div class="form-status {{ apiStatus == 'ok' ? 'up' : 'down' }}"
           data-toggle="tooltip" data-placement="top" title="Synchronisation active">
        <i class="fa fa-{{ apiStatus == 'ok' ? 'spinner fa-pulse' : 'times' }}"></i> Form {{ apiStatus == 'ok' ? 'online' : 'offline' }}
      </div>
    </div>

    <div class="col-xs-6">
      <div id="time-tracker" data-toggle="tooltip" data-placement="top" title="Temps passÃ© sur ce formulaire">
        <i class="fa fa-clock-o"></i> {{ min }} {{ sec }}
      </div>
    </div>

    <div class="col-xs-5">
      <div class="time-spend">
        <time-spend></time-spend>
      </div>
    </div>
  </div>

  <hr>

  <div class="row m-t">
    <div class="col-xs-6">
      <div class="time-tracker" data-toggle="tooltip" data-placement="top" title="Date d'anniversaire">
        <i class="fa fa-birthday-cake"></i> {{ age }} ans
      </div>
    </div>
  </div>

  <div class="row m-t">
    <div class="col-xs-12">
      <div class="time-tracker"><i class="fa fa-user"></i> Information</div>
    </div>
  </div>

  <div class="row m-t">
    <div class="col-xs-12">
      <div class="time-tracker">

      </div>
    </div>
  </div>
</aside>
